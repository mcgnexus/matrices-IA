{
  "version": 2,
  "builds": [
    {
      "src": "api/process-matrix.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/process-matrix",
      "dest": "api/process-matrix.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}